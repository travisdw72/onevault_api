# =============================================================================
# ONE VAULT PRODUCTION ENVIRONMENT CONFIGURATION
# =============================================================================
# IMPORTANT: Copy this to .env and update with real values
# NEVER commit .env to git - it contains secrets!

# =============================================================================
# DATABASE CONFIGURATION (PRODUCTION)
# =============================================================================
DB_HOST=your-production-database-host.com
DB_PORT=5432
DB_NAME=one_vault_production
DB_USER=onevault_app_user
DB_PASSWORD=your-super-secure-password-here
DB_SSL_MODE=require

# =============================================================================
# APPLICATION SECURITY 
# =============================================================================
APP_SECRET_KEY=your-64-character-hex-secret-key-here
JWT_SECRET=your-jwt-secret-key-here
SESSION_SECRET=your-session-secret-key-here

# =============================================================================
# PRODUCTION DOMAINS
# =============================================================================
DOMAIN=yourdomain.com
BASE_URL=https://app.yourdomain.com
API_BASE_URL=https://api.yourdomain.com 